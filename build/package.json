{
	"name": "helplib-builder",
	"title": "helpLib Builder",
	"description": "",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"build": "parallelshell \"npm run build-node\" \"npm run build-es6\" \"npm run build-es6.min\"",
		"build-node": "gulp --eng=node",
		"build-es6": "gulp --eng=es6",
		"build-es6.min": "gulp --eng=es6 --min",
		"watch": "parallelshell \"npm run watch-nodejs\" \"npm run watch-es6\"",
		"watch-nodejs": "gulp watch --eng=nodejs --min=0",
		"watch-es6": "gulp watch --eng=es6 --min=0"
	},
	"devDependencies": {
		"browserify": "^16.5.0",
		"gulp": "^4.0.1",
		"gulp-concat": "^2.6.1",
		"gulp-cssnano": "^2.1.3",
		"gulp-embed-svg": "^1.1.3",
		"gulp-file-include": "^2.0.1",
		"gulp-foreach": "^0.1.0",
		"gulp-htmlmin": "^5.0.1",
		"gulp-if": "^2.0.2",
		"gulp-jsonminify": "^1.1.0",
		"gulp-minify": "^3.1.0",
		"gulp-rename": "^1.4.0",
		"gulp-svgmin": "^2.2.0",
		"gulp-tap": "^2.0.0",
		"gulp-template": "^5.0.0",
		"gulp-zip": "^4.2.0",
		"merge-stream": "^1.0.1",
		"minimist": "^1.2.0",
		"parallelshell": "^3.0.2",
		"rimraf": "^2.6.3"
	}
}
