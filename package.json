{
	"name": "helplib",
	"title": "helpLib",
	"description": "",
	"version": "0.0.3",
	"scripts": {
		"build": "concurrently \"npm run build-node\" \"npm run build-es6\" \"npm run build-es6.min\"",
		"build-node": "npx gulp --gulpfile ./build/gulpfile.js --eng=node",
		"build-es6": "npx gulp --gulpfile ./build/gulpfile.js --eng=es6",
		"build-es6.min": "npx gulp --gulpfile ./build/gulpfile.js --eng=es6 --min",
		"test-node": "npx gulp --gulpfile ./test/gulpfile.js --eng=node",
		"test-node1": "npm run build-node -- --conf=\"../test/config/build-node.json\" && npx mocha \"./test/src/node/index.js\"",
		"test-es6": "npm run build-es6 -- --conf=\"../test/config/build-es6.json\"",
		"test-es6.min": "npm run build-es6.min -- --conf=\"../test/config/build-es6.json\""
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"concurrently": "^5.0.0",
		"gulp": "^4.0.1",
		"gulp-file": "^0.4.0",
		"gulp-if": "^2.0.2",
		"gulp-inlining-node-require": "^0.1.1",
		"gulp-matcha": "^1.2.0",
		"gulp-minify": "^3.1.0",
		"gulp-rename": "^1.4.0",
		"gulp-run-command": "0.0.9",
		"gulp-template": "^5.0.0",
		"merge-stream": "^1.0.1",
		"minimist": "^1.2.0",
		"mocha": "^6.2.2",
		"rimraf": "^2.6.3",
		"slash": "^3.0.0"
	}
}
