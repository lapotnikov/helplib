{
	"name": "helplib",
	"title": "helpLib",
	"description": "",
	"version": "1.0.0",
	"main": "dist/node/main.js",
	"scripts": {
		"build": "concurrently \"npm run build-node\" \"npm run build-es6\" \"npm run build-es6.min\"",
		"build-node": "npx gulp --gulpfile ./build/gulpfile.js --eng=node",
		"build-es6": "npx gulp --gulpfile ./build/gulpfile.js --eng=es6",
		"build-es6.min": "npx gulp --gulpfile ./build/gulpfile.js --eng=es6 --min",
		"test-node": "npx gulp --gulpfile ./test/gulpfile.js --eng=node",
		"test-es6": "npx gulp --gulpfile ./test/gulpfile.js --eng=es6",
		"test-es6.min": "npx gulp --gulpfile ./test/gulpfile.js --eng=es6 --min",
		"docs": "concurrently \"npm run docs-node\" \"npm run docs-es6\"",
		"docs-node": "npx gulp --gulpfile ./docs/gulpfile.js --eng=node",
		"docs-es6": "npx gulp --gulpfile ./docs/gulpfile.js --eng=es6"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"concurrently": "^5.0.0",
		"gulp": "^4.0.1",
		"gulp-file": "^0.4.0",
		"gulp-if": "^2.0.2",
		"gulp-inlining-node-require": "^0.1.1",
		"gulp-jsdoc3": "^3.0.0",
		"gulp-minify": "^3.1.0",
		"gulp-mocha": "^7.0.2",
		"gulp-open": "^3.0.1",
		"gulp-rename": "^1.4.0",
		"gulp-shell": "^0.7.1",
		"gulp-template": "^5.0.0",
		"jsdoc": "^3.6.6",
		"merge-stream": "^1.0.1",
		"minimist": "^1.2.3",
		"mocha": "^6.2.2",
		"rimraf": "^2.6.3",
		"slash": "^3.0.0"
	}
}
